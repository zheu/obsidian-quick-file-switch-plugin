{
	"name": "quick-file-switch",
	"version": "1.0.0",
	"description": "Obsidian plugin for quick file switching with custom shortcuts",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"file",
		"shortcut"
	],
	"author": "Your Name",
	"license": "MIT",
	"dependencies": {
		"@types/node": "^20.14.2",
		"@typescript-eslint/eslint-plugin": "^7.13.0",
		"@typescript-eslint/parser": "^7.13.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.21.5",
		"obsidian": "^1.6.7",
		"tslib": "^2.6.3",
		"typescript": "^5.4.5"
	}
}
